{
  "name": "@cosmjs/cosmwasm-stargate",
  "version": "0.39.0",
  "description": "Old name of @cosmjs/cosmwasm. Use @cosmjs/cosmwasm instead.",
  "contributors": [
    "Simon Warta <webmaster128@users.noreply.github.com>"
  ],
  "license": "Apache-2.0",
  "exports": {
    "types": "./build/index.d.ts",
    "default": "./build/index.js"
  },
  "files": [
    "build/",
    "*.md",
    "!*.spec.*",
    "!*testutils.*",
    "!**/karma-bundle/",
    "!**/testdata/"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cosmos/cosmjs/tree/main/packages/cosmwasm-stargate"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "docs": "typedoc",
    "format": "prettier --write --log-level warn \"./src/**/*.ts\"",
    "format-text": "prettier --write \"./*.md\"",
    "build": "rm -rf ./build && tsc",
    "build-or-skip": "[ -n \"${SKIP_BUILD:-}\" ] || yarn build",
    "test-node": "exit 0",
    "test-firefox": "exit 0",
    "test-chrome": "exit 0",
    "test": "yarn build-or-skip && yarn test-node",
    "coverage": "exit 0"
  },
  "dependencies": {
    "@cosmjs/cosmwasm": "workspace:^"
  },
  "devDependencies": {
    "@types/node": "*",
    "glob": "^13.0.0",
    "prettier": "^3.6.2",
    "typedoc": "^0.28",
    "typescript": "~5.9"
  }
}
